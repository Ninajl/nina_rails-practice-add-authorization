<div class="page-header">
  <h1>My Peeps!</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr>
      <td>
        <%= link_to student.name, student %>
      </td>
      <td>
        <%= student.email %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
